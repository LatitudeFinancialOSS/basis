{
  "name": "basis",
  "version": "2.0.0-beta.30",
  "description": "Basis Design System",
  "keywords": [
    "design-system",
    "react",
    "components",
    "library",
    "emotion"
  ],
  "bugs": {
    "url": "https://github.com/moroshko/basis/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moroshko/basis"
  },
  "license": "MIT",
  "author": "Misha Moroshko <misha.moroshko@latitudefinancial.com>",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "pre-commit": [
    "prettier",
    "lint"
  ],
  "scripts": {
    "start": "concurrently --kill-others \"yarn build-watch\"  \"yarn start-website\"",
    "start-website": "cd website && yarn clean && yarn start",
    "prettier": "prettier --write \"**/*.{ts,tsx,js,json,css,md}\"",
    "lint": "eslint --max-warnings 0 --ext .js,.ts,.tsx .",
    "test": "cross-env BABEL_ENV=cjs jest",
    "percy": "percy snapshot public -c .percy.js",
    "prebuild-basis": "rimraf dist",
    "build-basis": "rollup -c",
    "build-watch": "rollup -c -w",
    "prebuild": "rimraf public",
    "build": "cd website && yarn build && mv public .. && cd ..",
    "prepublishOnly": "yarn build-basis",
    "postinstall": "(test -d website && yarn install-website && husky install) || true",
    "install-website": "cd website && yarn && cd .."
  },
  "dependencies": {
    "@appsignal/javascript": "1.3.12",
    "@appsignal/react": "1.0.7",
    "date-fns": "2.21.1",
