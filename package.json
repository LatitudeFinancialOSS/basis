{
  "name": "basis",
  "version": "2.0.0-beta.0",
  "description": "Basis Design System",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "concurrently --kill-others \"npm run build-watch\"  \"npm run start-website\"",
    "start-website": "cd website && npm run clean && npm start",
    "prettier": "prettier --write \"**/*.{js,json,css,md}\"",
    "lint": "eslint --max-warnings 0 --ext .js,.ts,.tsx .",
    "test": "cross-env BABEL_ENV=cjs jest",
    "percy": "percy snapshot public -c .percy.js",
    "prebuild-basis": "rimraf dist",
    "build-basis": "rollup -c",
    "build-watch": "rollup -c -w",
    "prebuild": "rimraf public",
    "build": "cd website && npm run build && mv public .. && cd ..",
    "prepublishOnly": "npm run build-basis",
    "postinstall": "(test -d website && npm run install-website && husky install) || true",
    "install-website": "cd website && npm install && cd .."
  },
  "dependencies": {
    "date-fns": "2.19.0",
    "deep-object-diff": "1.1.0",
    "deepmerge": "4.2.2",
    "downshift": "5.4.7",
    "klona": "2.0.4",
    "mem": "6.1.1",
    "nanoid": "3.1.22",
    "polished": "3.7.1",
    "react-keyed-flatten-children": "1.3.0"
  },
  "peerDependencies": {
    "@emotion/react": "^11.1.5",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.13.10",
    "@babel/core": "7.13.10",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/preset-env": "7.13.10",
    "@babel/preset-react": "7.12.13",
    "@emotion/babel-preset-css-prop": "11.2.0",
    "@emotion/jest": "11.2.1",
    "@emotion/react": "11.1.5",
    "@percy/agent": "0.28.6",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "17.1.0",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "10.4.9",
    "@testing-library/react-hooks": "3.7.0",
    "@testing-library/user-event": "12.8.3",
    "@types/jest": "26.0.22",
    "@typescript-eslint/parser": "4.19.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "concurrently": "6.0.0",
    "cross-env": "7.0.3",
    "eslint": "7.22.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-jest-dom": "3.6.5",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "3.10.2",
    "husky": "5.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "mq-polyfill": "1.1.8",
    "prettier": "2.2.1",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-test-renderer": "16.14.0",
    "rimraf": "3.0.2",
    "rollup": "2.42.4",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-typescript2": "0.30.0",
    "ts-jest": "26.5.4",
    "tslib": "2.1.0",
