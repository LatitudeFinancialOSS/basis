    "@typescript-eslint/eslint-plugin": "4.27.0",
    "@typescript-eslint/parser": "4.27.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "concurrently": "6.0.2",
    "cross-env": "7.0.3",
    "eslint": "7.25.0",
    "eslint-import-resolver-alias": "1.1.2",
