    "rimraf": "3.0.2",
    "rollup": "2.46.0",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-dts": "3.0.2",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-typescript2": "0.30.0",
    "semver": "7.3.5",
    "ts-jest": "27.0.3",
    "tslib": "2.2.0",
